<%= form_for @song do |f| %>
    <%= f.label "Title" %>
    <%= f.text_field :title %><br>

    <%= f.label "Genre" %>
    <%= f.collection_select(:genre_id, Genre.all, :id, :name) %><br>
    
    <%= f.label "Image" %>
    <%= f.text_field :image %><br>

    <%= f.label "Link to Song" %>
    <%= f.text_field :sound_link %><br>

    <%= f.label "Link to Music Video" %>
    <%= f.text_field :video_link %><br>

    <%= f.submit %>
<% end %>